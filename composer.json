{
    "name": "saritasa/laravel-testbed",
    "type": "library",
    "description": "Laravel Testbed",
    "keywords": ["php", "Saritasa"],
    "homepage": "https://github.com/Saritasa/php-laravel-testbed",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\LaravelTestbed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Saritasa\\LaravelTestbed\\Tests\\": "tests/" }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
